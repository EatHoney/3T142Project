page{
    background: #f5f6fa;
}
font{
  font-size: 26rpx;
}
image{
  size: 60rpx;
  width:60rpx;
  height: 60rpx;
  margin: 2px 0 0 2px;
}
button{
  border: 0;
  color: red;
  display: flex;
  height: 100rpx;
  border-radius:0;
  font-size: 28rpx;
  align-items: center;
  justify-content: center;
  background-color: #fff;
}

button::after {
  border: none;
  border-radius: 0;
}

.arrow_wrap {
    position: absolute;
    width: 50px;
    height: 45px;
    right: 0;
    top: 0;
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: 16px;
    background-position: 17px 14px;
}